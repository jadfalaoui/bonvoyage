{
    "version": 2,
    "builds": [{
      "src": "vercel_entry.py",
      "use": "@vercel/python"
    }],
    "routes": [{
      "src": "/(.*)",
      "dest": "vercel_entry.py"
    }],
    "env": {
      "DATABASE_URL": "@database_url"
    },
    "build": {
      "env": {
        "DEBUG": "false"
      }
    }
  }
  